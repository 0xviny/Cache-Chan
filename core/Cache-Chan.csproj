<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <RootNamespace>Cache_Chan</RootNamespace>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <PackageId>CacheChan.Core</PackageId>
    <Version>1.2.1</Version>
    <Authors>0xviny</Authors>
    <Company>0xviny</Company>
    <Description>Uma biblioteca de cache flexível com suporte a políticas de expiração e LRU.</Description>
    <PackageTags>cache; lru; memorycache; dotnet; redis</PackageTags>
    <RepositoryUrl>https://github.com/0xviny/Cache-Chan</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageProjectUrl>https://github.com/0xviny/Cache-Chan</PackageProjectUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <PackageReleaseNotes>Primeira versão estável com suporte a políticas de expiração e LRU.</PackageReleaseNotes>
    <IsPackable>true</IsPackable>
    <UserSecretsId>a499316c-035b-4737-b605-1016d6e9aaa4</UserSecretsId>
  </PropertyGroup>
  <ItemGroup>
    <None Include="../README.md" Pack="true" PackagePath="" />
    <None Include="../LICENSE" Pack="true" PackagePath="" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="9.0.4" />
    <PackageReference Include="Microsoft.Extensions.Options" Version="9.0.4" />
  </ItemGroup>
</Project>